<launch>

<include file="$(find gazebo_ros)/launch/empty_world.launch">
</include>

<!-- Spawn a robot into Gazebo -->
<param name="robot_description" command="$(find xacro)/xacro.py '$(find allegro_hand_description)/urdf/allegro_hand.xacro'" />
<node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model ahand" />


</launch>
