<?xml version="1.0"?>
<launch>

	<arg name="controller" default="TaskInverseKinematics" doc="Name of the controller to be loaded (e.g. TaskInverseKinematics)" />
	<arg name="stopped_controllers" default="" doc="Additional controllers to be loaded, but not started (can be started later with controller_manager)" />

	<!-- Load joint controller configurations from YAML file to parameter server -->
	<rosparam file="$(find ahand_controllers)/config/ahand_controllers.yaml" command="load"/>

	<!--node ns="ahand" name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen"  args="TaskInverseKinematics"/-->
	<node ns="ahand" name="controller_spawner_js" pkg="controller_manager" type="spawner" respawn="false" output="screen" args="joint_state_controller"/>


	<!--node ns="ahand" name="controller_spawner_stopped" pkg="controller_manager" type="spawner" respawn="false" output="screen"  args="-stopped $(arg stopped_controllers)"/-->


</launch>
